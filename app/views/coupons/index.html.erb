<h1>All Coupons</h1>
<div>
    <table>
        <tr>
            <th>Coupon code</th>
            <th>Store name</th>
        </tr>
        <tr>
          <% @coupons.each do |coupon| %>
            <td><%= link_to coupon.coupon_code, coupon_path(coupon) %></td>
            <td><%= coupon.store %></td>
        </tr>
        <% end %>
    </table>
</div>
<p><%= link_to "Create New Coupon", new_coupon_path %></p>